  "license": "GPL-3.0",
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{js,jsx}": [
        "eslint --no-ignore --fix",
        "git add --force"
      ],
      "*.{json,md,graphql}": [
        "prettier --write",
        "git add --force"
      ],
      "*.{css,less,styl,scss,sass,sss}": [
        "stylelint --fix",
        "git add --force"
      ]
    }
  },
  "scripts": {
    "dev": "yarn run development",
    "development": "webpack --mode development",
    "watch": "yarn run development --watch",
    "prod": "yarn run production",
    "production": "webpack --mode production"
  }
